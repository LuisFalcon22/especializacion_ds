from abc import ABC, abstractmethod


class Volador(ABC):
    @abstractmethod
    def volar(self):
        pass

class Nadador(ABC):
    @abstractmethod
    def nadar(self):
        pass

class Caminante(ABC):
    @abstractmethod
    def caminar(self):
        pass

class Pajaro(Volador):
    def volar(self):
        print("Volando alto en el cielo")


class Pez(Nadador):
    def nadar(self):
        print("Nadando en el agua")


class Perro(Caminante):
    def caminar(self):
        print("Caminando en el suelo")